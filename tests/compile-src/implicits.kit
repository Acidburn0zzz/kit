function f(a: Int, b: Float, c: CString) {
    printf("hello %s!\n", c);
}

function main() {
    var a: Int = 1;
    var b: Float = 2.0;
    var c: CString = "world2";

    using implicit a {
        f(3.5, "world");
    }

    // implicit order shouldn't matter
    using implicit b, implicit a {
        f("world");

        // nested implicit
        using implicit c f();
    }
}
