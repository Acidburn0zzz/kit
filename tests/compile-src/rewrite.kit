include "stdio.h";

rules MyRewriteRules {
    (5) => 6;
    (${x: Int} %% 1) => $x;
    (apple) => true;
    (banana) => strawberry;
}

function main() {
    printf("%li\n", 5);

    using rules MyRewriteRules {
        // custom operator; must be rewritten or will fail to compile
        var x: Int = 5;
        printf("%li\n", x);
        // x %% 1;

        // var doesn't exist; must be rewritten or will fail to compile
        if (apple) {
            printf("apple => true\n");
        }

        var strawberry = true;
        if (banana) {
            printf("banana => strawberry\n");
        }
    }
}
